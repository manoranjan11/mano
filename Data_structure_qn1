#include <iostream>
#include<stdio.h>
#include <stdlib.h>
using namespace std;

struct node
{ string url;
  struct node *next;	
}*p;

node* end = NULL;

void newurl()
{
  string url;
  cout<<"Enter the url : ";
  cin>>url;
  p = new node;
  p->url = url;
  if(end == NULL)
   p->next =  NULL;
  else
   p->next = end;

   end = p;		
}

void goback()
{
	if(end==NULL)
		cout<<"No Websites to show!!!";
	else
	   {p=end; 
        end=end->next;
        delete p;
       }
}

int main()
{ 
	int opt;
	while(1)
	{
		cout<<"\n\n\"FOO Browser\"!\n\n\n";
        if(end!=NULL)
	         cout<<"Welcome to "<<end->url;
	cout<<"\n1.Enter a Url\n2.Go back\n3.Exit\n";
	cout<<"Enter the option:";
	cin>>opt;

		switch(opt)
		{ case 1: newurl();
				  break;
		  case 2: goback();
				  break;
	  	  case 3: exit(0);
	  	  default: cout<<"\nEnter a valid option!";
		  		  
	    }
    }	
return 0;
}
